                </article>
            </article>
        </article>

        <article class="item ">
            <img class="secound-slide " src="http://www.hrmengineering.se/files/img/karin_hansen_kompetensomraden.jpg " alt="Secound slide " ">
            <article class="container">
                <article class="carousel-caption">
                    <h1>Kom och jobba</h1>
                    <p> Se vår fina röda bil</p>
                    <p class="btn btn-lg btn-primary" href="#" role="button">Läs mera</p>
                </article>
            </article>
        </article>

        <article class="item">
            <img class="third-slide" src="http://www.hrmengineering.se/files/img/hrm_toppbild_anders_magnusson_svensk.jpg" alt="Third slide" ">
            <article class="container ">
                <article class="carousel-caption ">
                    <h1>Mer info</h1>
                    <p> Lorem ipsum  Lorem ipsum ipsum happsum</p>
                    <p class="btn btn-lg btn-primary " href="# " role="button">Knapp</p>
                </article>
            </article>
        </article>
    </article>
    <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
        <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
        <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
    </a>

</section>


<section class="container marketing ">
    <article class="row ">
        @{

            foreach (var item in Model)
            {
                if (item.Visas)
                {
                    string s = "Details/" + item.PostID.ToString();
                    <article class="col-lg-4 col-md-6 col-xs-12">
                      
                        @if (item.PictureURL != null)
                        {
                            <img src="@item.PictureURL" alt="Alternate Text" class="img-circle" width="160" height="160" />
                        }
                       
                        <h2>@item.Title</h2>
                        <p>@item.ShortText</p>
                        <p><strong>@item.Arthur</strong></p>
                        <input type="button" class="btn btn-info" value="Let's Go!" onclick="location.href='@Url.Action(s, "Posts")'" />

                    </article>
                }
            }
        }
        </article>

</section>
<section>
